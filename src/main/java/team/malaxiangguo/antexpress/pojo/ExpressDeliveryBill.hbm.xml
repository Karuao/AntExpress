<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="team.malaxiangguo.antexpress.pojo.ExpressDeliveryBill" table="express_delivery_bill"
           schema="antexpress">
        <id name="expressDeliveryBillId">
            <column name="express_delivery_bill_id" sql-type="int(11)"/>
        </id>
        <property name="expressDeliveryBillNo">
            <column name="express_delivery_bill_no" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
        <property name="status">
            <column name="status" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
        <property name="senderId">
            <column name="sender_id" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="receiverId">
            <column name="receiver_id" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="createDateTime">
            <column name="create_date_time" sql-type="datetime" not-null="true"/>
        </property>
        <property name="modifyDateTime">
            <column name="modify_date_time" sql-type="datetime" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>