<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="team.malaxiangguo.antexpress.pojo.Employee" table="employee" schema="antexpress">
        <id name="employeeId">
            <column name="employee_id" sql-type="int(11)"/>
        </id>
        <property name="account">
            <column name="account" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
        <property name="password">
            <column name="password" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
        <property name="gender">
            <column name="gender" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
        <property name="phoneNo">
            <column name="phone_no" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
        <property name="email">
            <column name="email" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
        <property name="salary">
            <column name="salary" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="departmentId">
            <column name="department_id" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="occupationId">
            <column name="occupation_id" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="createDateTime">
            <column name="create_date_time" sql-type="datetime" not-null="true"/>
        </property>
        <property name="modifyDateTime">
            <column name="modify_date_time" sql-type="datetime" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>