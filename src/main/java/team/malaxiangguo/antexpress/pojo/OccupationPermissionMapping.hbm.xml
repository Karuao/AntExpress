<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="team.malaxiangguo.antexpress.pojo.OccupationPermissionMapping" table="occupation_permission_mapping"
           schema="antexpress">
        <id name="occupationPermissionMappingId">
            <column name="occupation_permission_mapping_id" sql-type="int(11)"/>
        </id>
        <property name="occupationId">
            <column name="occupation_id" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="permissionId">
            <column name="permission_id" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="createDateTime">
            <column name="create_date_time" sql-type="datetime" not-null="true"/>
        </property>
        <property name="modifyDateTime">
            <column name="modify_date_time" sql-type="datetime" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>