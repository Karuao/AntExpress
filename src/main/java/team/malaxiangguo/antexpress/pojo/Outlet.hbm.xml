<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="team.malaxiangguo.antexpress.pojo.Outlet" table="outlet" schema="antexpress">
        <id name="outletId">
            <column name="outlet_id" sql-type="int(11)"/>
        </id>
        <property name="address">
            <column name="address" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
        <property name="deliveryAddress">
            <column name="delivery_address" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
        <property name="departmentId">
            <column name="department_id" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="createDateTime">
            <column name="create_date_time" sql-type="datetime" not-null="true"/>
        </property>
        <property name="modifyDateTime">
            <column name="modify_date_time" sql-type="datetime" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>