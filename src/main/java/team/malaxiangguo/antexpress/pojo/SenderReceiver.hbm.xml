<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="team.malaxiangguo.antexpress.pojo.SenderReceiver" table="sender_receiver" schema="antexpress">
        <id name="senderReceiverId">
            <column name="sender_receiver_id" sql-type="int(11)"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
        <property name="phoneNo">
            <column name="phone_no" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
        <property name="telephoneNo">
            <column name="telephone_no" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
        <property name="address">
            <column name="address" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
        <property name="completeAddress">
            <column name="complete_address" sql-type="text" not-null="true"/>
        </property>
        <property name="createDateTime">
            <column name="create_date_time" sql-type="datetime" not-null="true"/>
        </property>
        <property name="modifyDateTime">
            <column name="modify_date_time" sql-type="datetime" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>